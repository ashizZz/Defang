{
  "manifest_version": 3,
  "name": "Defang IP and URL",
  "version": "1.0",
  "description": "Double-click on URLs or IPs to defang within an overlay.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "images/icon48.png"
  }
}
